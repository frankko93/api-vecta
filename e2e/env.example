# E2E Test Environment Variables
# Copy to .env: cp e2e/env.example e2e/.env

# API
API_HOST=server
API_PORT=3080
API_READ_HEADER_TIMEOUT=10s
API_GRACEFUL_TIMEOUT=10
API_REQUEST_LOG=false
API_RUN_SWAGGER=false

# Database
DB_DRIVER=postgres
DB_HOST=postgres
DB_PORT=5432
DB_USER=go8_e2e_user
DB_PASS=go8_e2e_pass
DB_NAME=go8_e2e_db
DB_SSLMODE=disable
DB_MAX_CONNECTION_POOL=10
DB_MAX_IDLE_CONNECTIONS=5
DB_CONNECTIONS_MAX_LIFE_TIME=5m

# CORS
CORS_ALLOWED_ORIGINS=*

